                  "code": {
                    "type": "string"
                  }
                },
                "required": [
                  "email",
                  "code"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK (access + refresh tokens)"
          },
          "401": {
            "description": "Invalid code"
          }
        }
      }
    },
