    "/v1/auth/request": {
      "post": {
        "summary": "Request login code",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string"
                  }
                },
                "required": [
                  "email"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Invalid request"
          }
        }
      }
    },
    "/v1/auth/verify": {
      "post": {
        "summary": "Verify login code",
        "requestBody": {
          "required": true,
