{
  "name": "livenew",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build:assets": "node scripts/version-assets.js && node scripts/verify-assets.mjs",
    "prestart": "npm run build:assets",
    "start": "node src/server/index.js",
    "start:app": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "push": "git add -A && git commit -m 'Update' && git push origin main",
    "deploy": "npm run push",
    "test:integration": "node scripts/integration.test.js",
    "test:golden": "node scripts/golden.snapshots.js",
    "test:load": "node scripts/loadtest.js",
    "test:migrations": "node scripts/migrations.test.js",
    "test:mvp": "node scripts/mvp.unit.test.js",
    "test:constraints": "node scripts/constraints.coverage.test.js",
    "test:safeguards": "node scripts/safeguards.test.js",
    "test:smoke": "node scripts/smoke.loop.js",
    "test": "npm run test:mvp && npm run test:integration && npm run test:golden && npm run test:migrations && npm run test:constraints && npm run test:safeguards",
    "simulate": "node scripts/simulate.js",
    "db:profile": "node scripts/db-profile.js",
    "retention:dry-run": "node scripts/retention-dry-run.js",
    "launch:check": "node scripts/launch-check.js",
    "nightly:canary": "node scripts/nightly-canary.js",
    "maintenance:weekly": "node scripts/maintenance-weekly.js",
    "load:test": "node scripts/load-test.js",
    "canary:lift": "node scripts/canary-lift.js",
    "kill-switch:verify": "node scripts/kill-switch-verify.js",
    "maintenance:verify": "node scripts/maintenance-verify.js",
    "launch:gate": "node scripts/launch-gate.js"
  },
  "dependencies": {
    "@deepgram/sdk": "^4.11.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.10.1",
    "@supabase/supabase-js": "^2.95.3",
    "axios": "^1.13.5",
    "expo": "~54.0.32",
    "expo-status-bar": "~3.0.9",
    "fluent-ffmpeg": "^2.1.3",
    "openai": "^6.22.0",
    "react": "19.1.0",
    "react-dropzone": "^15.0.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "uuid": "^13.0.0",
    "zustand": "^5.0.10"
  },
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/uuid": "^10.0.0"
  }
